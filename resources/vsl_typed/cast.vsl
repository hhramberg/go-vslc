def casting(a, b int, c, d float) int
begin
    var e int
    var f float
    e := a + c
    f := b + d
    e := 3
    print "a is", a, "b is", b, "c is", c, "d is", d
    f := foo(e, 0.3)
    print "f is", f
    return 0
end

def foo(x, y float) float
    return x*y