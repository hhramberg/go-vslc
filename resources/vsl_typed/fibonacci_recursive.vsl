def fibonacci_recursive( n int) int
begin
    var f int
    f := fibonacci_number ( n )
    print "Fibonacci number #", n, "is", f
    return 0    // Success!
end

def fibonacci_number( n int )int
begin
    var y int
    y := 0
    if n > 2 then
        y := fibonacci_number(n-1) + fibonacci_number(n-2) 
    else
        y := 1
    return y
end
